{
  "servers": {
    "Redis MCP Server": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "redis-mcp-server@latest",
        "redis-mcp-server",
        "--url",
        "redis://localhost:6379/0"
      ],
      "env": {
        "REDIS_PWD": "mypassword"
      }
    }
  }
}